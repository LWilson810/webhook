{
    "version": 2,
    "builds": [
      {
        "src": "server.js", 
        "use": "@now/node"
      }
    ],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/server.js" }
    ],
    "env": {
      "NODE_ENV": "production"
    },
     "functions": {
        "webhook/**/*.js": {
          "runtime": "nodejs12.x",
          "memory": 512
        }
      }

  }
